at is considered legacy. We recommend using instead the native Keras format, e.g. `model.save('my_model.keras')` or `keras.saving.save_model(model, 'my_model.keras')`. \n"]},{"output_type":"stream","name":"stdout","text":["\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\b\r\u001b[1m62/62\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m95s\u001b[0m 1s/step - accuracy: 0.6173 - loss: 1.0427 - val_accuracy: 0.6947 - val_loss: 0.8284\n","Epoch 2/10\n","\u001b[1m62/62\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m88s\u001b[0m 1s/step - accuracy: 0.6941 - loss: 0.8206 - val_accuracy: 0.6947 - val_loss: 0.8363\n","Epoch 3/10\n","\u001b[1m62/62\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m87s\u001b[0m 1s/step - accuracy: 0.6946 - loss: 0.8162 - val_accuracy: 0.6947 - val_loss: 0.8345\n","Epoch 4/10\n","\u001b[1m62/62\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m93s\u001b[0m 2s/step - accuracy: 0.6962 - loss: 0.7971 - val_accuracy: 0.6947 - val_loss: 0.8546\n","Epoch 5/10\n","\u001b[1m62/62\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m88s\u001b[0m 1s/step - accuracy: 0.6858 - loss: 0.8147 - val_accuracy: 0.6947 - val_loss: 0.8354\n","Epoch 6/10\n","\u001b[1m62/62\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m95s\u001b[0m 2s/step - accuracy: 0.6845 - loss: 0.8087 - val_accuracy: 0.6947 - val_loss: 0.8417\n","\n","✅ Transfer Learning model saved as tumour_transfer_model.h5\n"]}],"source":["# ✅ IMPORTS\n","import os\n","import tensorflow as tf\n","from tensorflow.keras.preprocessing.image import ImageDataGenerator\n","from tensorflow.keras import layers, models, optimizers\n","from tensorflow.keras.applications import MobileNetV2\n","from tensorflow.keras.callbacks import ModelCheckpoint, EarlyStopping\n","\n","print(\"✅ TensorFlow version:\", tf.__version__)\n","\n","# ✅ PATH SETUP\n","base_dir = \"/content/Tumour\"  # <-- ✅ CORRECT FOLDER\n","\n","print(\"\\n✅ Using base folder:\", base_dir)\n","\n","IMAGE_SIZE = (224, 224)\n","BATCH_SIZE = 32\n","\n","# ✅ IMAGE GENERATORS with validation split\n","train_datagen = ImageDataGenerator(\n","    rescale=1./255,\n","    validation_split=0.2\n",")\n","\n","train_generator = train_datagen.flow_from_directory(\n","    base_dir,\n","    target_size=IMAGE_SIZE,\n","    batch_size=BATCH_SIZE,\n","    class_mode='categorical',\n","    subset='training'\n",")\n","\n","valid_generator = train_datagen.flow_from_directory(\n","    base_dir,\n","    target_size=IMAGE_SIZE,\n","    batch_size=BATCH_SIZE,\n","    class_mode='categorical',\n","    subset='validation'\n",")\n","\n","# ---------------------------\n","# ✅ 1️⃣ CUSTOM CNN MODEL\n","# ---------------------------\n","custom_cnn = models.Sequential([\n","    layers.Conv2D(32, (3, 3), activation='relu', input_shape=(IMAGE_SIZE[0], IMAGE_SIZE[1], 3)),\n","    layers.MaxPooling2D(2, 2),\n","    layers.Conv2D(64, (3, 3), activation='relu'),\n","    layers.MaxPooling2D(2, 2),\n","    layers.Conv2D(128, (3, 3), activation='relu'),\n","    layers.MaxPooling2D(2, 2),\n","    layers.Flatten(),\n","    layers.Dense(128, activation='relu'),\n","    layers.Dropout(0.5),\n","    layers.Dense(train_generator.num_classes, activation='softmax')\n","])\n","\n","custom_cnn.compile(\n","    loss='categorical_crossentropy',\n","    optimizer=optimizers.Adam(),\n","    metrics=['accuracy']\n",")\n","\n","custom_cnn.summary()\n","\n","# ✅ Callbacks for Custom CNN\n","cnn_checkpoint = ModelCheckpoint(\"tumour_cnn_model.h5\", save_best_only=True)\n","early_stop = EarlyStopping(patience=5, restore_best_weights=True)\n","\n","# ✅ Train Custom CNN\n","EPOCHS = 10\n","\n","history_cnn = custom_cnn.fit(\n","    train_generator,\n","    epochs=EPOCHS,\n","    validation_data=valid_generator,\n","    callbacks=[cnn_checkpoint, early_stop]\n",")\n","\n","print(\"\\n✅ Custom CNN model saved as tumour_cnn_model.h5\")\n","\n","# ---------------------------\n","# ✅ 2️⃣ TRANSFER LEARNING MODEL\n","# ---------------------------\n","# Load MobileNetV2 base\n","base_model = MobileNetV2(\n","    input_shape=(IMAGE_SIZE[0], IMAGE_SIZE[1], 3),\n","    include_top=False,\n","    weights='imagenet'\n",")\n","base_model.trainable = False  # Freeze base layers\n","\n","transfer_model = models.Sequential([\n","    base_model,\n","    layers.GlobalAveragePooling2D(),\n","    layers.Dense(128, activation='relu'),\n","    layers.Dropout(0.5),\n","    layers.Dense(train_generator.num_classes, activation='softmax')\n","])\n","\n","transfer_model.compile(\n","    loss='categorical_crossentropy',\n","    optimizer=optimizers.Adam(),\n","    metrics=['accuracy']\n",")\n","\n","transfer_model.summary()\n","\n","# ✅ Callbacks for Transfer Learning\n","tl_checkpoint = ModelCheckpoint(\"tumour_transfer_model.h5\", save_best_only=True)\n","\n","# ✅ Train Transfer Learning Model\n","history_tl = transfer_model.fit(\n","    train_generator,\n","    epochs=EPOCHS,\n","    validation_data=valid_generator,\n","    callbacks=[tl_checkpoint, early_stop]\n",")\n","\n","print(\"\\n✅ Transfer Learning model saved as tumour_transfer_model.h5\")\n"]}]}